{
    "lithops": {
        "lithops": {
            "storage_bucket": "<EXISTING_COS_BUCKET>",
            "storage": "ibm_cos",
            "mode": "serverless",
            "include_modules": ["engine/sm"],
            "data_cleaner": true,
            "data_limit": false,
            "workers": 100,
            "execution_timeout": 2400
        },
        "serverless": {
            "backend": "code_engine",
            "runtime_timeout": 1200,
            "runtime_memory": 1024,
	    "runtime_cpu": 250 
        },
        "localhost": {},
        "ibm": {"iam_api_key": "<IAM_API_KEY>"},
	"code_engine": {
            "namespace": "<NAMESPACE>",
            "region": "<REGION>",
            "runtime": "kpavel/meta-ce-lithops-runtime:latest",
	    "runtime_memory": 1024,
	    "runtime_cpu": 0.25
	},
        "ibm_cos": {
            "region": "us-east"
        },
        "sm_storage": {
            "imzml": ["<EXISTING_COS_BUCKET>", "imzml"],
            "moldb": ["<EXISTING_COS_BUCKET>", "moldb"],
            "centroids": ["<EXISTING_COS_BUCKET>", "centroids"],
            "pipeline_cache": ["<EXISTING_COS_BUCKET>", "pipeline_cache"]
        }
    }
}
